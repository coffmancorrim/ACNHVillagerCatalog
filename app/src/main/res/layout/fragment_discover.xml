<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/layout_discover"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.fragments.DiscoverFragment"
    android:clickable="false"
    >


    <TextView
        android:id="@+id/text_discover"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:text="DISCOVER FRAGMENT" />

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/fragment_inner_container_view"
        app:layout_constraintTop_toBottomOf="@id/text_discover"
        app:layout_constraintStart_toStartOf="@id/text_discover"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        />

</androidx.constraintlayout.widget.ConstraintLayout>